{
  "name": "focal-deploy",
  "version": "2.0.0",
  "description": "Complete AWS deployment automation with wizard-based setup",
  "main": "bin/focal-deploy.js",
  "bin": {
    "focal-deploy": "./bin/focal-deploy.js"
  },
  "scripts": {
    "start": "node bin/focal-deploy.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node bin/focal-deploy.js",
    "build:pkg": "pkg . --config package-config.json",
    "build:electron": "electron-builder",
    "build:all": "npm run build:pkg && npm run build:electron",
    "package:macos": "pkg . --targets node18-macos-x64,node18-macos-arm64 --out-path dist/macos",
    "package:linux": "pkg . --targets node18-linux-x64,node18-linux-arm64 --out-path dist/linux",
    "package:windows": "pkg . --targets node18-win-x64 --out-path dist/windows",
    "package:all": "npm run package:macos && npm run package:linux && npm run package:windows",
    "prepare:release": "npm run test && npm run package:all"
  },
  "keywords": [
    "aws",
    "deployment",
    "cli",
    "ec2",
    "s3",
    "docker",
    "automation",
    "wizard",
    "setup",
    "infrastructure",
    "emergency-access"
  ],
  "author": "Focal Deploy Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.0.0",
    "@aws-sdk/client-ec2-instance-connect": "^3.911.0",
    "@aws-sdk/client-ecr": "^3.0.0",
    "@aws-sdk/client-iam": "^3.913.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-ssm": "^3.912.0",
    "@aws-sdk/client-sts": "^3.0.0",
    "@octokit/rest": "^20.0.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "fs-extra": "^11.0.0",
    "glob": "^11.0.3",
    "inquirer": "^8.2.0",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "node-forge": "^1.3.1",
    "ora": "^5.4.1",
    "simple-git": "^3.20.0",
    "ssh2": "^1.15.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/focal-deploy/focal-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/focal-deploy/focal-deploy/issues"
  },
  "homepage": "https://github.com/focal-deploy/focal-deploy#readme"
}
